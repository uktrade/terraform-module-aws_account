        "sagemaker:Describe*",
        "sagemaker:Get*",
        "sagemaker:List*",
        "sdb:Get*",
        "sdb:List*",
        "servicecatalog:Describe*",
        "servicecatalog:Get*",
        "servicecatalog:List*",
